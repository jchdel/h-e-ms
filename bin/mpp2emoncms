#!/usr/bin/python3
# EASY-INSTALL-ENTRY-SCRIPT: 'mpp-solar==0.4.6','console_scripts','mpp2emoncms'
__requires__ = 'mpp-solar==0.4.6'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('mpp-solar==0.4.6', 'console_scripts', 'mpp2emoncms')()
    )
